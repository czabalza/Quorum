<% question.tags().each(function (tag) { %>
  <div>
    <%= tag.escape("topic") %>
  </div>
<% }) %>
<h1><%= question.escape('title') %></h1>
<% if (question.escape('description')) { %>
  <h3><%= question.escape('description') %></h3>
<% } %>
<div id="new-answer">
<button class="new-answer-btn">Answer</button>
</div>
<div id="answers">
</div>
