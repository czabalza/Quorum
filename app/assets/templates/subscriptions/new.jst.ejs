<h1>Select topics you're interested in!</h1>

<form>
  <% tags.each(function (tag) { %>
    <label>
      <input type="checkbox" name="subscription[tag_ids][]" value="<%= tag.id %>">
      <%= tag.escape('topic') %>
    </label>
  <% }) %>

  <button class="submit-subs-btn btn btn-primary">Submit</button>
</form>
